{   
    "graph_name":"MovieDemo",
    "schema":"./demo/MovieDemo/schema/movie_type.txt", 
    "delimiter":",",
    "vertexs":[
    {
        "type_name":"personType",
        "path":"./demo/MovieDemo/csv/vertex_person.csv",
        "columns":[
            "id:ID","name","born","poster_image",":LABEL"
        ],
        "header":1,
        "primary":"id"
    },
    {
        "type_name":"userType",
        "path":"./demo/MovieDemo/csv/vertex_user.csv",
        "columns":[
            "id:ID","login",":LABEL"
        ],
        "header":1,
        "primary":"id"     
    },
    {
        "type_name":"genreType",
        "path":"./demo/MovieDemo/csv/vertex_genre.csv",
        "columns":[
            "id:ID","name",":LABEL"
        ],
        "header":1,
        "primary":"id"        
    },  
    {
        "type_name":"keywordType",
        "path":"./demo/MovieDemo/csv/vertex_keyword.csv",
        "columns":[
            "id:ID","name",":LABEL"
        ],
        "header":1,
        "primary":"id"        
    },
    {
        "type_name":"movieType",
        "path":"./demo/MovieDemo/csv/vertex_movie.csv",
        "columns":[
            "id:ID","title","tagline","summary","poster_image","duration","rated",":LABEL"
        ],
        "header":1,
        "primary":"id"       
    }
],
    "edges":[
    {
        "type_name":"acted_inType",
        "path":"./demo/MovieDemo/csv/edge_acted_in.csv",
        "columns":[
            ":START_ID",":END_ID","role",":TYPE"
        ],
        "header":1,
        "src_type_name":"personType",
        "dst_type_name":"movieType"
    },
    {
        "type_name":"rateType",
        "path":"./demo/MovieDemo/csv/edge_rate.csv",
        "columns":[
            ":START_ID","SKIP",":END_ID","stars",":TYPE"
        ],
        "header":1,
        "src_type_name":"userType",
        "dst_type_name":"movieType"
    },
    {
        "type_name":"directedType",
        "path":"./demo/MovieDemo/csv/edge_directed.csv",
        "columns":[
            ":START_ID",":END_ID",":TYPE"
        ],
        "header":1,
        "src_type_name":"personType",
        "dst_type_name":"movieType"
    },
    {
        "type_name":"is_friendType",
        "path":"./demo/MovieDemo/csv/edge_is_friend.csv",
        "columns":[
            ":START_ID",":END_ID",":TYPE"
        ],
        "header":1,
        "src_type_name":"userType",
        "dst_type_name":"userType"
    },
    {
        "type_name":"has_genreType",
        "path":"./demo/MovieDemo/csv/edge_has_genre.csv",
        "columns":[
            ":START_ID",":END_ID",":TYPE"
        ],
        "header":1,
        "src_type_name":"movieType",
        "dst_type_name":"genreType"
    },
    {
        "type_name":"has_keywordType",
        "path":"./demo/MovieDemo/csv/edge_has_keyword.csv",
        "columns":[
            ":START_ID",":END_ID",":TYPE"
        ],
        "header":1,
        "src_type_name":"movieType",
        "dst_type_name":"keywordType"
    },
    {
        "type_name":"produceType",
        "path":"./demo/MovieDemo/csv/edge_produce.csv",
        "columns":[
            ":START_ID",":END_ID",":TYPE"
        ],
        "header":1,
        "src_type_name":"personType",
        "dst_type_name":"movieType"
    },
    {
        "type_name":"writeType",
        "path":"./demo/MovieDemo/csv/edge_write.csv",
        "columns":[
            ":START_ID",":END_ID",":TYPE"
        ],
        "header":1,
